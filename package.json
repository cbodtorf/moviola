{
  "name": "moviola",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "nx run-many --target=serve --projects=backend,frontend",
    "start:be": "nx serve backend",
    "start:fe": "nx serve frontend",
    "start:prod": "concurrently \"yarn start:be:prod\" \"yarn start:fe:prod\"",
    "start:be:prod": "node ./dist/apps/backend/main.js",
    "start:fe:prod": "cd ./dist/apps/frontend && npx next start",
    "build:prod": "nx run-many --target=build --prod --projects=backend,frontend",
    "build:be:prod": "nx build backend --prod",
    "build:fe:prod": "nx build frontend --prod",
    "test": "nx run-many --target=test --projects=backend,frontend --parallel=2",
    "e2e:fe": "nx e2e frontend-e2e",
    "test:be": "nx test backend",
    "test:fe": "nx test frontend"
  },
  "private": true,
  "dependencies": {
    "@nrwl/next": "14.5.1",
    "algoliasearch": "^4.14.2",
    "core-js": "^3.6.5",
    "fastify": "^4.3.0",
    "next": "12.2.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-instantsearch-hooks-web": "^6.30.3",
    "reflect-metadata": "^0.1.13",
    "regenerator-runtime": "0.13.7",
    "rxjs": "^7.0.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
