    "@nrwl/jest": "14.5.1",
    "@nrwl/linter": "14.5.1",
    "@nrwl/nest": "^14.5.1",
    "@nrwl/node": "14.5.1",
    "@nrwl/nx-cloud": "latest",
    "@nrwl/react": "14.5.1",
    "@nrwl/web": "14.5.1",
    "@nrwl/workspace": "14.5.1",
    "@testing-library/react": "13.3.0",
    "@types/jest": "27.4.1",
    "@types/node": "16.11.7",
    "@types/react": "18.0.15",
    "@types/react-dom": "18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
